{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ClickHouseConnectionPoolConfiguration": {
    "ConnectionStrings": [
      "Host=Host;Port=Port;Database=default;User=default;Password="
    ],
    "ConnectionPoolMaxCount": 4,
    "ConnectionPoolName": "ClickHouseConnectionPool"
  },
  "ClickHouseAggregatingQueueProcessorConfiguration": {
    "MaxPackageSize": 10,
    "ProcessingThreadsCount": 2,
    "QueueMaxSize": 10,
    "TimerPeriodMs": 10000
  },
  "AllowedHosts": "*"
}
